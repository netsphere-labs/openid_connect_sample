<h1>New Client (RP)</h1>

<article>
  <%= form_for @client, :html => {:class => 'uk-form-horizontal'} do |f| %>
    <%= render 'clients/form', client:@client, redirect_uris:@redirect_uris,
                               f: f %>
    <%= link_to 'Cancel', '/dashboard', class:'uk-button uk-button-default' %>
    <%= f.submit class:'button is-primary' %>
  <% end %>
</article>

